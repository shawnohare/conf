for lig in "${ligs[@]}"; do
    echo "${lig}"
    printf "\n"
done

printf '\n\n'
echo "Printing styilzed text."
echo -e "\e[1mbold\e[0m"
echo -e "\e[3mitalic\e[0m"
echo -e "\e[4munderline\e[0m"
echo -e "\e[9mstrikethrough\e[0m"
echo -e "\e[4:3mundercurl\e[0m"


printf '\n\n'
echo "Printing true color test."
awk 'BEGIN{
    for (colnum = 0; colnum<255; colnum++) {
        r = 255-(colnum);
        g = (colnum*510/255);
        b = (colnum);
        if (g>255) g = 510-g;
        printf "\033[48;2;%d;%d;%dm", r,g,b;
        printf "\033[38;2;%d;%d;%dm", 255-r,255-g,255-b;
        printf " \033[0m", substr(s,colnum+1,1);
    }
}'
